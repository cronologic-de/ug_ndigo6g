</listitem><listitem><para><ref refid="structndigo6g12__init__parameters_1af4bb2b03283eafaee3d91ce58b4b5397" kindref="member">force_bitstream_update</ref> = <computeroutput>false</computeroutput></para>
</listitem><listitem><para><ref refid="structndigo6g12__init__parameters_1a067707582ac772dfd88dd5b85af45da5" kindref="member">partial_bitstream_size</ref> = 0</para>
